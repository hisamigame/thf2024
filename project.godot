config_version=4

[application]

config/name="Hisami block game"
run/main_scene="res://titleScreen.tscn"
boot_splash/image="res://title_screen.png"
config/icon="res://icon.png"

[autoload]

global="*res://global.tscn"

[display]

window/size/width=448
window/size/height=384
window/size/test_width=1344
window/size/test_height=1152
